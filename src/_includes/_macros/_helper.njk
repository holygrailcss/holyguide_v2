{% macro token(tokenType='', explicacion='',dark='') %}
<h3>{{ tokenType }}</h3>
<p class="p-xs">{{ explicacion }}</p>
<div class="resize-mini">
  <div class="resize-mini-content-narrow {{dark  }} ">
    {% include 'tokens/' + tokenType + '.njk' %}
  </div>
</div>
{% set standardcode %}
{% include 'tokens/' + tokenType + '.njk' %}
{% endset %}
<pre><code class=" ">{{standardcode }} </code></pre>
{% endmacro %}


{% macro insert(elementType='', elements) %}
<div class="code-view">
<div class="check-new">


    
</div>


  {% for element in elements %}
  <{{ elementType }} class="{{ element }}">{{ element }}</{{ elementType }}>
  {% endfor %}
</div>
{% set standardcode %}
{% for element in elements %}<{{ elementType }} class="{{ element }}">{{ element }}</{{ elementType }}>
{% endfor %} {% endset %}

