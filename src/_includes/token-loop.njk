
      <article class="mb-24">
          <a class="w-100" href="{{ component.url }}">
            <div class="rat-1-1 w-100">
              <div class="rat-content">
        


   {# {% include '_components/'+component.data.titulo +'.njk' %}  #}

  <img class="video-cover" width="100%" height="100%" src="{{ component.data.image }}" alt="{{ component.titulo }}">
              </div>
            </div>
                </a>
          <div class="article-text mt-24">
              <div class="mb-16">
                <h3 class="fluid-8-b"><a href="{{ component.data.titulo }}">{{ component.data.titulo }}  </a></h3>
                  {{ component.data.descripcion }}
              </div>
           
              {% for tag in component.data.tags %}
              <div class=" tag-info ">
                {{ tag }}
              </div>
              {% endfor %}
  

 
          </div>
      </article>
