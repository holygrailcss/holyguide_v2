---
layout: base.njk
---
{% import "_macros/_helper.njk" as helper %}
{% import "_macros/_insert.njk" as insert %}
<div class="container-fluid">
  {% include '_website/hero.njk' %}
</div>
<div class="main-content innermarkdown ">{{ content | safe }}


<div class="row">



  </div>

      <h3 class="h3"> Botón solido</h3>

{% call helper.insert(elementType='button',['btn btn-primary', 'btn btn-limited' , 'btn btn-feel', 'btn btn-blue']) %}
{% endcall %}
      <h3 class="h3"> Botón outline</h3>

{% call helper.insert(elementType='button',['btn btn-secondary', 'btn btn-trans', 'btn btn-secondary-feel' , 'btn btn-pantom']) %}
{% endcall %}
         <h3 class="h3"> Botón clean</h3>

{% call helper.insert(elementType='button',['btn btn-clear', 'btn btn-text', 'link-raw' , 'link-line']) %}
{% endcall %}



