
      <article class="mb-24">
          <a class="w-100" href="{{ token.url }}">
            <div class="rat-1-1 w-100">
              <div class="rat-content">
                <img class="video-cover" width="100%" height="100%" src="{{ token.image }}" alt="{{ token.data.imageAlt }}"
                  class="h1">
              </div>
            </div>
                </a>
