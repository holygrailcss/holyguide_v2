{% macro token(tokenType='', explicacion='',dark='') %}
<h3>{{ tokenType }}</h3>
<p class="p-xs">{{ explicacion }}</p>
<div class="resize-mini">
  <div class="resize-mini-content-narrow {{dark  }} ">
    {% include 'tokens/' + tokenType + '.njk' %}
  </div>
</div>
{% set standardcode %}
{% include 'tokens/' + tokenType + '.njk' %}
{% endset %}
<pre><code class=" ">{{standardcode }} </code></pre>
{% endmacro %}
{% macro element(elementType='', explicacion='',dark='') %}
<h3>{{ elementType }} </h3>
<p class="p-xs">{{ explicacion }}</p>
<div class="resize-mini">
  <div class="resize-mini-content-narrow {{dark  }} ">
    <a class="mb-16  {{ elementType }} " href=""> {{ elementType }}</a>
  </div>
</div>
{% set standardcode %}
<a class="mb-16  {{ elementType }} " href=""> {{ elementType }}</a>
{% endset %}
<pre><code class="language-markup">{{standardcode}} </code></pre>
{% endmacro %}




{% macro input(elementType='', explicacion='') %}
<h3>{{ elementType }} </h3>
<p class="p-xs">{{ explicacion }}</p>
<div class="mb-32 ">
  <div class="code-view ">
    <div class="check-new"> </div>
    <div class="checkbox-item-box">
      <div class="checkbox-item">
        <input id="checkbox_1" type="checkbox" />
        <label for="checkbox_1">
          <i class="icon">
            <svg width="16" height="16">
              <g stroke-width="1">
                <polyline fill="none" stroke="#000000" points="1,9 5,13 15,3 "></polyline>
              </g>
            </svg>
          </i>
        </label>
      </div>
    </div>
  </div>
  {% set standardcode %}
  <div class="checkbox-item-box">
    <div class="checkbox-item">
      <input id="checkbox_1" type="checkbox" />
      <label for="checkbox_1">
        <i class="icon">
          <svg width="16" height="16">
            <g stroke-width="1">
              <polyline fill="none" stroke="#000000" points="1,9 5,13 15,3 "></polyline>
            </g>
          </svg>
        </i>
      </label>
    </div>
  </div>
  {% endset %}
  <div class="code-zone">
    <pre><code class=" language-markup">{{standardcode }} </code></pre>
  </div>
</div>
{% endmacro %}



