<li>
      <article class="mb-24">
          <a class="w-100" href="{{ url }}">
            <div class="rat-1-1 w-100">
              <div class="rat-content">
                <img class="video-cover" width="100%" height="100%" src="{{ image }}" alt="{{ imageAlt }}"
                  class="h1">
              </div>
            </div>
                </a>
          <div class="article-text mt-24">
              <h3 class="fluid-8-b"><a href="{{ titulo }}">{{ titulo }}</a></h3>
              <p class="mb-8 p1 c-dark-grey">{{ date | postDate }} </span></p>
              
              
              {% for tag in elemento.data.tags %}
                  <div class=" tag-info ">
                    {{ tag }}
                  </div>
              {% endfor %}


              <p class="mb-24">{{description }}</p>
              <a href="{{url }}" class="btn link-line"> {{ titulo }}</a>
          </div>
      </article>
</li>